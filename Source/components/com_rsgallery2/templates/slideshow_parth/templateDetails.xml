	<params>
		<!-- Overrides: function startGallery() -->
		<!--  Automatically go to next image (after delay time): timed (true/false) -->
		<param name="automated_slideshow" type="list" label="COM_RSGALLERY2_SLIDESHOW_PARTH_AUTOMATED_SLIDESHOW" description="COM_RSGALLERY2_SLIDESHOW_PARTH_AUTOMATED_SLIDESHOW_DESC" >
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</param>
		<!--  Delay in milliseconds (6000 equals 6 seconds): delay -->
		<param name="delay" type="text" default="6000" label="COM_RSGALLERY2_SLIDESHOW_PARTH_DELAY" description="COM_RSGALLERY2_SLIDESHOW_PARTH_DELAY_DESC" size="20" />
		<param name="defaultTransition" type="list" default="fade" label="COM_RSGALLERY2_SLIDESHOW_PARTH_DEFAULTTRANSITION" description="COM_RSGALLERY2_SLIDESHOW_PARTH_DEFAULTTRANSITION_DESC" >
			<option value="fade">COM_RSGALLERY2_SLIDESHOW_PARTH_FADE</option>
			<option value="crossfade">COM_RSGALLERY2_SLIDESHOW_PARTH_CROSSFADE</option><!-- Real Cross-Fading for transparent PNG-->
			<option value="fadebg">COM_RSGALLERY2_SLIDESHOW_PARTH_FADEBG</option><!-- Fading to background -->
			<option value="fadeslideleft">COM_RSGALLERY2_SLIDESHOW_PARTH_FADESLIDELEFT</option><!-- Neat fade&slide effect -->
			<option value="continuoushorizontal">COM_RSGALLERY2_SLIDESHOW_PARTH_CONTINUOUSHORIZONTAL</option><!-- Continuous scrolling -->
			<option value="continuousvertical">COM_RSGALLERY2_SLIDESHOW_PARTH_CONTINUOUSVERTICAL</option><!-- Continuous scrolling -->
		</param>
		<!--  Show the thumbs carousel (at the top): showCarousel (true/false) -->
		<param name="showCarousel" type="list" default="1" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SHOWCAROUSEL" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SHOWCAROUSEL_DESC" >
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</param>
		<!--  Show the image information panel (at the bottom): showInfopane (true/false) -->
		<param name="showInfopane" type="list" default="1" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SHOWINFOPANE" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SHOWINFOPANE_DESC" >
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</param>
		<!--  Slide the image information panel (at the bottom): slideInfoZoneSlide (true/false) -->
		<param name="slideInfoZoneSlide" type="list" default="1" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONESLIDE" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONESLIDE_DESC" >
			<option value="true">JYES</option>
			<option value="0">JNO</option>
		</param>
		<!--  Show the arrows, left and right: showArrows (true/false) -->
		<param name="showArrows" type="list" default="1" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SHOWARROWS" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SHOWARROWS_DESC" >
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</param>
		<!--  Disable the 'open image' link for the images: embedLinks: false  -->
		<param name="embedLinks" type="list" default="1" label="COM_RSGALLERY2_SLIDESHOW_PARTH_EMBEDLINKS" description="COM_RSGALLERY2_SLIDESHOW_PARTH_EMBEDLINKS_DESC" >
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</param>
		<!--  Thumbnail height: thumbHeight  -->
		<param name="thumbHeight" type="text" default="50" label="COM_RSGALLERY2_SLIDESHOW_PARTH_THUMBHEIGHT" description="COM_RSGALLERY2_SLIDESHOW_PARTH_THUMBHEIGHT_DESC" size="20" />
		<!--  Thumbnail width: thumbWidth -->
		<param name="thumbWidth" type="text" default="50" label="COM_RSGALLERY2_SLIDESHOW_PARTH_THUMBWIDTH" description="COM_RSGALLERY2_SLIDESHOW_PARTH_THUMBWIDTH_DESC" size="20" />
		<!--  Fade duration in milliseconds (500 equals 0.5 seconds): fadeDuration -->
		<param name="fadeDuration" type="text" default="500" label="COM_RSGALLERY2_SLIDESHOW_PARTH_FADEDURATION" description="COM_RSGALLERY2_SLIDESHOW_PARTH_FADEDURATION_DESC" size="20" />
		<!--  Text on the carousel tab: textShowCarousel-->
		<param name="textShowCarousel" type="textarea" default="" label="COM_RSGALLERY2_SLIDESHOW_PARTH_TEXTSHOWCAROUSEL" description="COM_RSGALLERY2_SLIDESHOW_PARTH_TEXTSHOWCAROUSEL_DESC" />
		<!-- Override default CSS styles -->
		<!-- Slideshow full width (when 0: then largest width of display image -->
		<param name="slideshowWidth" type="text" default="0" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDESHOWWIDTH" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDESHOWWIDTH_DESC" size="20" />
		<!-- Slideshow full height (when 0: then largest height of display image -->
		<param name="slideshowHeight" type="text" default="0" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDESHOWHEIGHT" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDESHOWHEIGHT_DESC" size="20" />
		<!-- Background color for the slideshow element (CSS background-color Property)-->
		<param name="slideshowBackgroundcolor" type="text" default="#000000" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDESHOWBACKGROUNDCOLOR" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDESHOWBACKGROUNDCOLOR_DESC" size="20" />
		<!-- Carousel backgroundcolor (CSS background-color Property), color item title, height (px) -->
		<param name="carouselHeight" type="text" default="135" label="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELHEIGHT" description="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELHEIGHT_DESC" size="20" />
		<param name="carouselBackgroundcolor" type="text" default="#333333" label="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELBACKGROUNDCOLOR" description="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELBACKGROUNDCOLOR_DESC" size="20" />
		<param name="carouselTextcolor" type="text" default="#FFFFFF" label="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELTEXTCOLOR" description="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELTEXTCOLOR_DESC" size="20" />
		<!-- Carousel color numberlabel -->
		<param name="carouselNumberlabelColor" type="text" default="#B5B5B5" label="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELNUMBERLABELCOLOR" description="COM_RSGALLERY2_SLIDESHOW_PARTH_CAROUSELNUMBERLABELCOLOR_DESC" size="20" />
		<!-- slideInfoZone background color (CSS background-color Property), height (px) -->
		<param name="slideInfoZoneHeight" type="text" default="60" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONEHEIGHT" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONEHEIGHT_DESC" size="20" />
		<param name="slideInfoZoneBackgroundcolor" type="text" default="#333333" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONEBACKGROUNDCOLOR" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONEBACKGROUNDCOLOR_DESC" size="20" />
		<!-- slideInfoZone text color -->
		<param name="slideInfoZoneTextcolor" type="text" default="#EEEEEE" label="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONETEXTCOLOR" description="COM_RSGALLERY2_SLIDESHOW_PARTH_SLIDEINFOZONETEXTCOLOR_DESC" size="20" />
	</params>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
