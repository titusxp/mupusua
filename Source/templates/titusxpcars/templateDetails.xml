<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 1.6//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/1.6/template-install.dtd">
<extension version="2.5" type="template" client="site">
	<name>TitusXP Cars</name>
	<creationDate>August 2013</creationDate>
	<author>Titus Yusinyu</author>
	<authorEmail>titusyusinyu@gmail.com</authorEmail>
	<authorUrl>http://www.titusxp.tk</authorUrl>
	<copyright>Copyright (C) 2005 - 2011 Open Source Matters, Inc. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<version>2.5</version>
	<description>Car template re - formatted for joomla</description>

	<files>
		<folder>css</folder>
		<folder>js</folder>
		<folder>images</folder>
		<filename>index.html</filename>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<filename>favicon.ico</filename>
	</files>

	<positions>
		<position>main-menu</position>
		<position>company_title</position>
		<position>slide-show</position>
		<position>user1</position>
		<position>user2</position>
		<position>user3</position>
		<position>right</position>
		<position>padding</position>
		<position>right-bottom</position>
		<position>special</position>
		<position>footer</position>
	</positions>
	
	<!-- 	For core templates, we also install/uninstall the language files in the core language folders.
	-->

	
	
</extension>
